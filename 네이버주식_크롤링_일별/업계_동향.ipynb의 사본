","        print(f\"âŒ Error: {e}\")\n","        return None\n","\n","# ë„¤ì´ë²„ ë‰´ìŠ¤ ê²€ìƒ‰ API í˜¸ì¶œ í•¨ìˆ˜ (ìµœëŒ€ 100ê°œë§Œ ê°€ì ¸ì˜¤ê¸°)\n","def get_naver_search(keyword):\n","    base = \"https://openapi.naver.com/v1/search/news.json\"\n","    parameters = f\"?query={quote(keyword)}&start=1&display=100&sort=date\"  # ìµœì‹ ìˆœ ì •ë ¬\n","    url = base + parameters\n","\n","    response = get_request_url(url)\n","    if response is None:\n","        return None\n","    else:\n","        return json.loads(response)\n","\n","# ë‰´ìŠ¤ ë°ì´í„° í•„í„°ë§ í•¨ìˆ˜\n","def filter_news(post, keywords):\n","    \"\"\"\n","    ê¸°ì‚¬ ì œëª©ì´ë‚˜ ì„¤ëª…ì— ì£¼ì–´ì§„ í‚¤ì›Œë“œ ë¦¬ìŠ¤íŠ¸ì˜ ë‹¨ì–´ê°€ í¬í•¨ë˜ê³ ,\n","    EXCLUDE_KEYWORDS(ì œì™¸í•  í‚¤ì›Œë“œ)ê°€ í¬í•¨ë˜ì§€ ì•Šì€ ê²½ìš°ë§Œ True ë°˜í™˜\n","    \"\"\"\n","    print()\n","    title = post['title']\n","    description = post['description']\n","\n","    # í¬í•¨í•  í‚¤ì›Œë“œ ì²´í¬ (í•˜ë‚˜ë¼ë„ í¬í•¨ë˜ë©´ OK)\n","    if not any(keyword in title or keyword in description for keyword in keywords):\n","        return False  # ê´€ë ¨ í‚¤ì›Œë“œê°€ í¬í•¨ë˜ì§€ ì•Šì€ ê¸°ì‚¬\n","\n","    # ì œì™¸í•  í‚¤ì›Œë“œ ì²´í¬ (í•˜ë‚˜ë¼ë„ í¬í•¨ë˜ë©´ False)\n","    if any(keyword in title or keyword in description for keyword in EXCLUDE_KEYWORDS):\n","        return False  # ì œì™¸í•  í‚¤ì›Œë“œê°€ í¬í•¨ëœ ê¸°ì‚¬ì´ë¯€ë¡œ í•„í„°ë§\n","\n","    return True  # ê´€ë ¨ í‚¤ì›Œë“œê°€ í¬í•¨ë˜ë©°, ì œì™¸í•  í‚¤ì›Œë“œê°€ í¬í•¨ë˜ì§€ ì•Šì€ ê²½ìš°\n","\n","# ê²€ìƒ‰ ì‹¤í–‰ í•¨ìˆ˜\n","def main():\n","\n","    all_news = []  # ëª¨ë“  ì£¼ì œì˜ ë‰´ìŠ¤ë¥¼ ì €ì¥í•  ë¦¬ìŠ¤íŠ¸\n","    total_cnt = 0  # ì „ì²´ ë‰´ìŠ¤ ê°œìˆ˜ ì¹´ìš´íŠ¸\n","\n","    for topic, keywords in related_keywords.items():\n","        json_result = []\n","        cnt = 0\n","\n","        # API ìš”ì²­\n","        json_response = get_naver_search(topic)\n","\n","        if json_response is None or 'items' not in json_response:\n","            print(f\"âŒ '{topic}'ì— ëŒ€í•œ ë‰´ìŠ¤ ê²€ìƒ‰ ê²°ê³¼ê°€ ì—†ìŠµë‹ˆë‹¤.\")\n","            continue\n","\n","        total = json_response['total']\n","        print(f\"ğŸ” '{topic}' ì „ì²´ ê²€ìƒ‰ ê²°ê³¼: {total}ê±´ (ìµœëŒ€ 100ê°œ ê²€ìƒ‰)\")\n","\n","        # ë‰´ìŠ¤ ê°€ì ¸ì˜¤ê¸° ë° ë‚ ì§œ + í‚¤ì›Œë“œ í•„í„°ë§\n","        for post in json_response['items']:\n","            # ë‚ ì§œ ë³€í™˜ (YYYYMMDD)\n","            p_date = datetime.datetime.strptime(post['pubDate'], '%a, %d %b %Y %H:%M:%S +0900')\n","            p_date = p_date.strftime('%Y%m%d')\n","\n","            # ì…ë ¥í•œ ë‚ ì§œì™€ ì¼ì¹˜í•˜ê³ , ê´€ë ¨ í‚¤ì›Œë“œ í¬í•¨ & ë¶ˆí•„ìš”í•œ ë‰´ìŠ¤ ì œì™¸\n","            if p_date == target_date and filter_news(post, keywords):\n","                cnt += 1\n","                json_result.append({\n","                    'cnt': cnt,\n","                    'topic': topic,  # ì£¼ì œëª… ì¶”ê°€\n","                    'title': post['title'],\n","                    'description': post['description'],\n","                    'org_link': post['originallink'],\n","                    'link': post['link'],\n","                    'pDate': p_date\n","                })\n","\n","        # í•„í„°ë§ëœ ë‰´ìŠ¤ë§Œ ë¦¬ìŠ¤íŠ¸ì— ì¶”ê°€\n","        all_news.extend(json_result)\n","        total_cnt += len(json_result)\n","\n","    # JSON ì €ì¥\n","    filename = f\"Filtered_News_{target_date}.json\"\n","    with open(filename, 'w', encoding='utf8') as outfile:\n","        json.dump(all_news, outfile, indent=4, sort_keys=True, ensure_ascii=False)\n","\n","    print(f\"âœ… ì´ ê°€ì ¸ì˜¨ ë°ì´í„°: {total_cnt}ê±´\")\n","    print(f\"ğŸ“‚ ì €ì¥ ì™„ë£Œ: {filename}\")\n","\n","    # ê²°ê³¼ë¥¼ DataFrameìœ¼ë¡œ ë³€í™˜í•˜ì—¬ CSV ì €ì¥\n","    df = pd.DataFrame(all_news)\n","    if not df.empty:\n","        df.to_csv(f\"Filtered_News_{target_date}.csv\", encoding='utf-8-sig', index=False)\n","        print(f\"ğŸ“‚ CSV ì €ì¥ ì™„ë£Œ: Filtered_News_{target_date}.csv\")\n","        display(df)\n","    else:\n","        print(\"âŒ í•´ë‹¹ ë‚ ì§œì— ëŒ€í•œ ê´€ë ¨ 